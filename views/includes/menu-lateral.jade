div(flex="33" ,layout="row" layout-align="start start")
  md-fab-speed-dial(md-open="false", md-direction="down", ng-class="md-scale")
    md-fab-trigger
      md-button(aria-label="Contato", class="md-fab md-primary md-accent")
        md-icon(md-svg-src="images/icons/ic_menu_48px.svg")
    md-fab-actions
      md-button(ui-sref="lerpost", aria-label="Posts", md-no-ink class="md-primary md-raised md-accent")
        span Posts
      md-button(ui-sref="tutoriais", aria-label="Tutoriais", md-no-ink class="md-primary md-raised md-accent")
        span Tutoriais
      md-button(ui-sref="contato", aria-label="Contato", md-no-ink class="md-primary md-raised md-accent")
        span Contato
